pipeline {
agent any
tools { maven 'maven_3.8.3' }
stages{
 stage('Git checkout'){
  steps{
  checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[credentialsId: 'github-credentials', url: 'https://github.com/kcbrown401/jenkins-project1.git']])
      }
    }
    stage('Run validate'){
    steps{
     bat 'mvn validate'
  }
 }
  stage('Run compile'){
     steps{
      bat 'mvn compile'
      }
    }
 }
}